<h2 id="title"><a href="/">Movies:</a></h2>
<p><a href="/actors">Go to Actor Listing</a></p>
<table>
  <tr class="top">
    <td><a href="/movies?order=title">Title</a></td>
    <td><a href="/movies?order=year">Year</a></td>
    <td><a href="/movies?order=rating">Rating</a></td>
    <td>Genre</td>
    <td>Studio</td>
  </tr>
  <% @movies.each do |title| %>
    <tr>
      <td><a href="/movies/<%= title["id"] %>"><%= title["title"] %></a></td>
      <td><%= title["year"] %></td>
      <td><%= title["rating"] %></td>
      <td><%= title["genre"] %></td>
      <td><%= title["studio"] %></td>
    </tr>
  <% end %>
</table>
